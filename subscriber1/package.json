{
  "name": "datacollection",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SET NODE_ENV=production && nodemon ./src/app.ts",
    "test2": "SET NODE_ENV=test && npm run migration && jest",
    "test": "SET NODE_ENV=test && ts-node  --transpile-only ./src/test/setup/migration.ts && jest --forceExit",
    "dev": "SET NODE_ENV=development && nodemon ./src/app.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npx eslint src/** --fix",
    "migration": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:gen": "npm run typeorm:cli -- migration:generate -n"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/moment": "^2.13.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^4.4.1",
    "http-errors": "~1.6.3",
    "prettier-eslint": "^12.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "save": "^2.4.0",
    "ts-jest": "^26.5.5",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.31",
    "typeorm-typedi-extensions": "^0.4.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.30",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
